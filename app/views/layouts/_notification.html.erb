<div class="toast" role="alert" aria-live="assertive" aria-atomic="true" data-path="<%= notifications_poll_path %>" data-notifications_path="<%= notifications_users_path %>">

  <div class="toast-header">
    <strong class="mr-auto"></strong>
    <small></small>
    <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  <div class="toast-body"></div>

</div>

<%= javascript_pack_tag 'poll_notification', 'data-turbolinks-track': 'reload' %>
